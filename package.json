{
  "name": "EmbeddedPages",
  "version": "1.0.1",
  "description": "This mashup reflects the pages you've specified in URL or Template URL custom fields as an embedded page on an entity view.",
  "keywords": [
    "webpack",
    "targetprocess",
    "mashup"
  ],
  "scripts": {
    "start": "node scripts/startWatcher.js",
    "build:development": "node scripts/buildDevelopment.js",
    "build:library:docs": "node scripts/buildLibraryDocs.js",
    "build:library:scripts": "node scripts/buildLibraryScripts.js",
    "build:library": "npm run build:library:scripts && npm run build:library:docs",
    "build:manager": "node scripts/buildManager.js",
    "build:all": "npm run build:library && npm run build:manager"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.11.4",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^0.8.5",
    "postcss-loader": "^0.8.0",
    "shelljs": "^0.7.3",
    "style-loader": "^0.13.0",
    "targetprocess-mashup-helper": "^0.4.1",
    "targetprocess-mashup-webpack-plugin": "^1.0.0",
    "webpack": "^1.12.12"
  },
  "dependencies": {
    "cancelable-promise": "^2.4.0"
  },
  "author": "Targetprocess <dev@targetprocess.com>",
  "license": "MIT",
  "contributors": [
    "Aliaksei Shytkin <shitkin@targetprocess.com>"
  ]
}
